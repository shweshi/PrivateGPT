{
  "build": {
    "env": {
      "NODE_ENV": "production",
      "NODE_VERSION": "18.14.0"
    },
    "output_directory": "out",
    "command": "npm run build"
  },
  "static_sites": [
    {
      "name": "private-gpt-client",
      "directory": "/client",
      "publish": {
        "branch": "main",
        "command": "npm install && npm run build",
        "public_directory": "out",
        "env": {
            "API_URL": "https://privategpt-br2e.onrender.com"
        }
      }
    }
  ]
}
